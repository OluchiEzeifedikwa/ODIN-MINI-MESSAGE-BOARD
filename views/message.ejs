<nav>
    <ul>
      <% for (let i = 0; i < messages.length; i++) { %>
        <li>
          <div class="container"><%= messages[i].text %>
            <%= messages[i].user %> 
            <span> <%= messages[i].added %> </span></div>
          <a href="/new/ <%= messages[i].href %>">ADD A NEW MESSAGE</a>
          <span> <%= messages[i].text %> </span>
          <span> <%= messages[i].user %> </span>
          <span> <%= messages[i].added %> </span>
        </li>
      <% } %>
    </ul>
</nav>
  


